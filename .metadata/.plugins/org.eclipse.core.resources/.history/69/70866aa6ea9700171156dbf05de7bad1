
public class Main {
	public void main(String[] args){
		
	};
	//nada demais
	private String tipoTriangulo(int lado1, int lado2, int lado3){
		
		if (lado1>lado2+lado3||lado2>lado1+lado3||lado3>lado1+lado2){
			int aux=0;
			if(lado1==lado2){
				aux++;
			}
			if (lado2 == lado3){
				aux++;
			}
			if (lado3==lado1){
				aux++;
			}
		}else{
			return "Triangulo nao existe";
		}
		return "";
	};
}
