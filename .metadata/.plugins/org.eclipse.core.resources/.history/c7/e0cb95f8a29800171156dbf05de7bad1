import static org.junit.Assert.*;

import org.junit.Test;

import junit.framework.TestCase;

public class DivisoresTest extends TestCase {
	int numero;
	public DivisoresTest(int numero){
		this.numero = numero;
	}
	@Test
	public String retornaDivisores(){
		String divisores = "";
		
		for (int i=1; i<=numero;i++){
			if (numero%i==0){
				divisores +=i+" ";
			}
		}
		return divisores;
	}

}
